{
  "name": "auto-server-client",
  "version": "0.1.1",
  "description": "A unified, secure data-access layer for Next.js App Router that eliminates the need to write individual Server Actions for every API request",
  "keywords": [
    "nextjs",
    "next.js",
    "app-router",
    "server-actions",
    "api-client",
    "react-hooks",
    "ssr",
    "typescript",
    "authentication",
    "proxy"
  ],
  "author": "Prabal Goyal",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/prabal-goyal/next-server-client.git"
  },
  "homepage": "https://github.com/prabal-goyal/next-server-client#readme",
  "bugs": {
    "url": "https://github.com/prabal-goyal/next-server-client/issues"
  },
  "private": false,
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsup",
    "prepublishOnly": "npm run build",
    "lint": "eslint"
  },
  "dependencies": {
    "next": "16.1.1",
    "react": "19.2.3",
    "react-dom": "19.2.3"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "16.1.1",
    "tailwindcss": "^4",
    "tsup": "^8.5.1",
    "typescript": "^5"
  }
}